    "    axes[i].axvline(mean_val, color='red', linestyle='--', label=f'Mean: {mean_val:.2f}')\n",
    "    axes[i].legend()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# 4. Bar Charts\n",
    "print(\"\\n4. Bar Charts\")\n",
    "\n",
    "fig, axes = plt.subplots(2, 2, figsize=(15, 10))\n",
    "\n",
    "# Default rate by grade\n",
    "grade_defaults = df.groupby('grade')['loan_status'].mean()\n",
