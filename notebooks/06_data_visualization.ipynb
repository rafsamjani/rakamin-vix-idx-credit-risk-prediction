    "\n",
    "# Basic dataset information\n",
    "print(f\"\\nDataset Overview:\")\n",
    "print(f\"- Total Records: {len(df):,}\")\n",
    "print(f\"- Default Rate: {(df['loan_status'] == 1).mean():.2%}\")\n",
    "print(f\"- Number of Features: {len(df.columns)}\")\n",
