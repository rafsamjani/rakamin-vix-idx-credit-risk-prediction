    "line2 = ax2.plot(range(len(monthly_loan_amnt)), monthly_loan_amnt.values, \n",
    "                 label='Loan Amount', color='green')\n",
    "ax1.set_xlabel('Time Period')\n",
    "ax1.set_ylabel('Avg Interest Rate (%)', color='blue')\n",
    "ax2.set_ylabel('Avg Loan Amount ($)', color='green')\n",
    "ax1.set_title('Interest Rate and Loan Amount Over Time')\n",
    "ax1.legend(loc='upper left')\n",
    "ax2.legend(loc='upper right')\n",
    "\n",
    "# Grade distribution over time\n",
    "grade_time_series = df_time.groupby(['month', 'grade']).size().unstack(fill_value=0)\n",
    "for grade in grade_time_series.columns:\n",
    "    axes[1, 1].plot(range(len(grade_time_series)), grade_time_series[grade].values, \n",
    "                    label=f'Grade {grade}', alpha=0.7)\n",
    "axes[1, 1].set_title('Grade Distribution Over Time')\n",
    "axes[1, 1].set_xlabel('Time Period')\n",
    "axes[1, 1].set_ylabel('Number of Loans')\n",
    "axes[1, 1].legend()\n",
    "axes[1, 1].grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# 3. Histograms\n",
    "print(\"\\n3. Histograms\")\n",
    "\n",
    "fig, axes = plt.subplots(2, 3, figsize=(18, 12))\n",
