    "plt.ylabel('Number of Loans')\n",
    "for i, v in enumerate(status_counts.values):\n",
    "    plt.text(i, v + max(status_counts.values)*0.01, str(v), ha='center')\n",
    "\n",
    "plt.subplot(1, 3, 3)\n",
    "grade_default_rates = df.groupby('grade')['loan_status'].mean()\n",
    "plt.bar(grade_default_rates.index, grade_default_rates.values, \n",
    "        color=sns.color_palette(\"Reds_r\", len(grade_default_rates)))\n",
    "plt.title('Default Rate by Grade')\n",
    "plt.ylabel('Default Rate')\n",
    "plt.xlabel('Grade')\n",
    "for i, v in enumerate(grade_default_rates.values):\n",
    "    plt.text(i, v + 0.01, f'{v:.2f}', ha='center', fontsize=9)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ],
   "outputs": [],
